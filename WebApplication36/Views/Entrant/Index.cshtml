@model IEnumerable<WebApplication36.Data.Entrant>

@{
    Layout = null;
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" />
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p style="margin-top:30px;">
        <a asp-action="Create" class="btn btn-primary"><i class="fas fa-plus"></i>Create New</a>
    </p>
    @if (!Model.Any())
    {
        <div class="alert-danger p-3">
            <i class="fas fa-exclamation-triangle"></i>"No Data Found"
        </div>
    }
    else
    {
        <table class="table table-striped table-hover text-center">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.CategoryName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantDescription)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantPhotoVideoURL)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantFacebookPageUrl)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantTwitterHandle)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantInstagramHandle)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantYouTubeChannelUrl)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantFullAddress)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantGooglePlaceId)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantPhoneNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EntrantWebsiteUrl)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NumberOfWriteIns)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NumberOfVotes)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Status)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.CategoryName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantDescription)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantPhotoVideoURL)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantFacebookPageUrl)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantTwitterHandle)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantInstagramHandle)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantYouTubeChannelUrl)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantFullAddress)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantGooglePlaceId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantPhoneNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EntrantWebsiteUrl)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NumberOfWriteIns)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NumberOfVotes)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Status)
                        </td>

                    </tr>
                }
            </tbody>
        </table>
    }
</body>
</html>
